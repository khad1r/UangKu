<IfModule mod_rewrite.c>
    RewriteEngine On

    # Skip existing files/directories
    RewriteCond %{REQUEST_FILENAME} !-f
    RewriteCond %{REQUEST_FILENAME} !-d

    # Send everything else to index.php
    # RewriteRule ^ index.php [QSA,L]
    RewriteRule ^(.*)$ index.php/$1 [L]

    # Options -Indexes
</IfModule>
